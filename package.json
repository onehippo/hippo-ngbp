    "autoprefixer": "6.3.6",
    "babel-core": "6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
