{
  "name": "hippo-ngbp",
  "author": "Joeri de Gooijer",
  "private": true,
  "description": "Boilerplate for Hippo B.V. angular projects",
  "scripts": {
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "dev": "rimraf dist; webpack --bail --progress --profile --config webpack.config.dev.js",
    "release": "rimraf dist; webpack --config webpack.config.js",
    "server": "webpack-dev-server --content-base dist/ --config webpack.config.dev.js --history-api-fallback --hot --inline --port 9090"
  },
